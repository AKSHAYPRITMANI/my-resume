<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.myresume">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ProfilePhoto"
                  android:label="Profile Photo">
        </activity>
        <activity android:name="com.example.myresume.WorkExperience"
                  android:label="Skills">
        </activity>
        <activity android:name="com.example.myresume.Education"
                  android:label="Education">
        </activity>
        <activity android:name="com.example.myresume.MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>

</manifest>